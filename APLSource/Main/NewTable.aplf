 NewTable←{
     ⍝ ⍵ ←→ Namepace
     ⍝      ⍵.Titles (vec of char vecs)
     ⍝      ⍵.Values (Matrix of char vecs)
     ⍝      ⍵.Classes (vec of char vecs)
     ⍝ ← ←→ Table
     newRow←{⍺ New'tr'}
     newCell←{New'td'⍵}
     t←New'table'
     th←t New'thead'
     tr←th newRow 0
     hc←tr{⍺ New'th'}¨⍵.Titles
     hc.class←⍵.Classes
     tb←t New'tbody'
     cd←newCell¨⍵.Values
     r←tb newRow¨⍳≢⍵.Values
     r.Content←↓⍵.Values
     r.Content.class←⊂⍵.Classes
     t
 }
