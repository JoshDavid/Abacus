 ReadCSVFile←{
     ⍝ ⍵ ←→ FQP file name
     ⍝ ← ←→ Namespace with Names, Values, etc.
     o←'Invert' 1
     v n←(⎕CSV⍠o)⍵'' 1 1
     p←ConvertType¨v
     c←≢n
     t←⎕NS''
     t.Names←n
     t.Values←⍉↑{2=⍴⍴⍵:A.ntbs ⍵ ⋄ ⍵}¨p.Value
     ⍝ t.Values←p.Value
     t.Types←p.Type
     t.Align←p.Align
     t.SortDirection←c⍴0
     t.AutoCompleteItems←c⍴⊂''
     t
 }
