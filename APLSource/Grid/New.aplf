 New←{
     ⍝ ⍺ ←→ Parent
     ⍝ ⍵ ←→ Table Data
     ⍝   ←→
     ⍺←⊣
     ⎕THIS.H←##.Main
     t←⍺ H.NewTable ⍵
     t.id←GetAutoID 0
     t.Changed←0
     t.CurCell←0 0
     t.Context←⎕THIS
     t.Onclick←'OnClick'
     t.Oninput←'OnInput'
     t.Onkeydown←'OnKeyDown'
     t.class←'tcgrid'
     c←H.BodyCells t
     c.tabIndex←'0'
     c.class←⊂'notcurcell'
     c[0;0].class←'curcell'
     t.Style←BuildCSS t
     t
 }
